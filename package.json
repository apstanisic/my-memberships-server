  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix src --ext .js,.ts",
    "build": "rimraf dist && tsc",
    "start": "ts-node  dist/main.js",
    "dev": "nodemon",
    "debug": "node --inspect-brk dist/main.js",
    "sync-schema": "ts-node ./node_modules/.bin/typeorm schema:sync",
    "test-data": "ts-node ./node_modules/.bin/typeorm migration:run",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "dev-hmr": "ts-node dist/main.js",
    "webpack": "webpack --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/testing": "^6.8.5",
    "@nestjs/typeorm": "^6.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "faker": "^4.1.0",
    "moment": "^2.24.0",
    "nestjs-extra": "https://github.com/apstanisic/nestjs-extra",
    "query-string": "^6.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
